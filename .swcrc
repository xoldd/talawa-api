// More information at this link:- https://swc.rs/docs/configuration/swcrc
{
	"$schema": "https://swc.rs/schema.json",
	"jsc": {
		"baseUrl": "./",
		"keepClassNames": false,
		"loose": false,
		"parser": {
			"decorators": false,
			"dynamicImport": false,
			"syntax": "typescript",
			"tsx": false
		},
		"preserveAllComments": false,
		"target": "esnext"
	},
	"minify": true,
	"module": {
		"lazy": false,
		"noInterop": false,
		"resolveFully": true,
		"strict": true,
		"strictMode": true,
		"type": "nodenext"
	},
	"sourceMaps": false
}
